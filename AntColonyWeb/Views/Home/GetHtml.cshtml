@model IEnumerable<AntColonyWeb.Models.City>
@{
    ViewBag.Title = "GetHtml";
}

<h2>GetHtml</h2>

@Html.BeginForm(){  
<table class="table">
    <tr>
        <th>City name</th>
        <th>Cost</th>
        <th>Time to visit</th>
        <th>Value</th>
    </tr>
            @foreach (var city in Model)
            {
<tr>
    <td>@city.Name</td>
    <td>@Html.EditorFor(m => city.Cost)</td>
    <td>@Html.EditorFor(m => city.Time)</td>
    <td>@Html.EditorFor(m => city.Value)</td>
</tr>
            }

</table>
<input class="btn-1" style="width:200px; margin-top: 30px;" type="submit" />
}

<h3>Ціна на @ViewBag.FuelType:</h3>
<h4>@ViewBag.FuelPrice</h4>


